{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.samsonzhang.blurryface",
  "version": "0.1.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/public/",
  "url": "/",
  "enableServer": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "modes": {
    "window": {
      "title": "Blurryface",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/public/icon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\""
    }
  },
  "cli": {
    "binaryName": "blurryface",
    "resourcesPath": "/public/",
    "clientLibrary": "/public/neutralino.js",
    "binaryVersion": "4.14.1",
    "clientVersion": "3.12.0"
  }
}